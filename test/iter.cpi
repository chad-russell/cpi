type Range struct {
    start: i64,
    end: i64
}

fn until(end: i64) {
    return range(0, end);
}

fn range(start: i64, end: i64) {
    return cast(Range) {start, end};
}

#attr(Range) {
    fn empty(r: Range) {
        return r.start >= r.end;
    }

    fn front(r: Range) {
        return r.start;
    }

    fn popFront(r: *Range) {
        r.start += 1;
    }
}
