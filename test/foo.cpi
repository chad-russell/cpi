fn main() {
    f := fn() {
        ret fn() {
            ret foo;
        };
    };

    a1 := f()()()();

    a2 := apply(inc, 1) + apply_overboard(inc, 0);

    a3 := ^^^^^^&&&^&&&&^&^^&&3;

    a4_helper := &&&&^&&&&&&&&&a3;
    a4 := ^^^^^^^^^^^^a4_helper;

    a5 := a5_fn();

