#import "printing": p;

module foo {
    type F struct { x: i64, y: i64 }

    scope F {
        fn getX(f: F) {
            p.print("foo: ");
            return f.x;
        }
    }
}

module bar {
    type B struct { x: i32, y: i32 }
}

scope bar.B {
    #import bar;

    fn getX(b: B) {
        p.print("bar: ");
        return b.x;
    }
}

fn main() {
    #import foo;
    f: F = {3, 6};
    -- f|getX|p.println;
    p.println(getX(f));

    b: bar.B = {4, 7};
    b|getX|p.println;

    return 0;
}

