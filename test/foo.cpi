fn main() {
    type Foo struct {
        value: i32,
        bar: *Bar
    };

    type Bar struct {
        value: i32,
        baz: Baz
    };

    type Baz struct {
        value: i32,
        qux: *Qux
    };

    type Qux struct {
        value: i32
    };

    foo: Foo;

    bar: Bar;

    baz: Baz;
    baz.value = 777;

    foo.bar = &bar;
    bar.baz = baz;

    foo.bar.baz.value = 123;
    ret bar.baz.value;
}
