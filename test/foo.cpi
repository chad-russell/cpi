fn main() {
    type Foo struct {
        value: i32,
        bar: *Bar
    };

    type Bar struct {
        value: i32,
        baz: Baz
    };

    type Baz struct {
        value: i32,
        qux: Qux
    };

    type Qux struct {
        value: i32
    };

    foo: Foo;
    bar: Bar;
    baz: Baz;
    qux: Qux;

    foo.bar = &bar;

    foo.bar.baz.qux.value = 3;

    assert(foo.bar.baz.qux.value == 3);

    ret 0;
}
