fn main() {
    x := {x: 3, y: 4};
    y := {x: 4, y: 5};
    z := {x: 5, y: 6};

    fn sumThingy(t: struct{x: i64, y: i64}) {
        return t.x + t.y;
    }

    a := 0;
    #for thingy: []{x, y, z} {
        a = a + sumThingy(thingy);
    }
    return a;
}

fn println(s: []i8) {
    puts(s); puts("\n");
}

fn assert(b: bool) {
    if b == false { panic(); }
}
