fn foo() {
    ret bar;
}

fn main() {
    f := fn() {
        ret fn() {
            ret foo;
        };
    };

    a1 := f()()()();

    a2 := apply(inc, 1) + apply_overboard(inc, 0);

    a3 := ^^^^^^&&&^&&&&^&^^&&3;

    a4_helper := &&&&^&&&&&&&&&a3;
    a4 := ^^^^^^^^^^^^a4_helper;

    a5 := a5_fn();

    a6 := (^^&&a5_fn)();

    a7 := (^&(fn() i32 { ret 3; }))();

    a8_helper1 := &&bad;
    a8_helper2 := &&bar;
    ^a8_helper1 = ^a8_helper2;
    a8 := (^^a8_helper1)();

    a9 := testA9();

    a10 := testStruct1();

    a11 := testStruct2();

    a12 := testNestedCall();

    a13 := testDerefParam();

    a14 := testBug1();

    a15 := testDoubleDeref();

    a16 := more_struct();

    a17 := test_S2_stuff();

    a18 := testSetFromParam();

    a19 := testSetter();

    a20 := testLinkedList();

    ret a1 + a2 + a3 + a4 + a5 + a6 + a7 + a8 + a9 + a10 + a11 + a12 + a13 + a14 + a15 + a16 + a17 + a18 + a19 + a20;
}

type linkedList struct {
    value: i32,
    next: *linkedList
}

fn testLinkedList() {
    l: linkedList;
    l.value = 3;
    l.next = &l;

    ret l.next.next.next.next.value;
}

fn set_y(p: *point, y: i32) {
    p.y = y;
    ret y;
}

fn testSetter() {
    p: point;
    p.x = bad();
    p.y = bad();
    wtf := set_y(^&&p, 3);
    ret p.y;
}

fn testSetFromParam() {
    a : i32 = 999;
    wtf := set(&a);
    ret a;
}

fn set(n: *i32) {
    ^n = 3;
    ret 999;
}

fn test_S2_stuff() {
    s: S2;
    sx: struct{p1: i32, p2: i32};

    sx.p1 = 999;
    sx.p2 = 2;

    s.x = &&sx;

    ret s.x.p2 + 1;
}

type S2 struct {
	x: **struct {
		p1: i32,
		p2: i32
	},
	y: fn() i32
}

fn more_struct() {
    s: S;
    psx := &&&s.x;
    psx.p2 = bad();

    new_sx : struct{ p1: i32, p2: i32 };
    new_sx.p2 = 3;

    ^^^psx = new_sx;

    ret s.x.p2;
}

fn testDoubleDeref() {
    p: point;
    p.y = 3;

    q := &p;
    q1 := ^q;
    q2 := ^q;

    ret p.y + q1.y - q2.y;
}

fn testA9Helper() {
    a9 := bad();
    a9_p := &&&a9;
    ^^^a9_p = 3;
    ret ^^^a9_p;
}

fn testA9() {
    ret testA9Helper();
}

