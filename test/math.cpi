#link "libc";

PI: f64 = 3.141592654;

fn sin(n: f64) f64
fn cos(n: f64) f64
fn floor(f: f64) f64

fn max(a: !T, b: T) {
    if a >= b { return a; }
    return b;
}

fn min(a: !T, b: T) {
    if a <= b { return a; }
    return b;
}

fn abs(a: !T) {
    if a < 0 {
        return -a;
    }

    return a;
}

fn quantize(n: f64, step: f64) {
    return floor(n / step) * step;
}
